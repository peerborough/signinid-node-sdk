{
  "name": "signinid",
  "version": "0.3.0",
  "description": "Node.js SDK for SigninID sandbox email testing API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "signinid",
    "email",
    "testing",
    "otp",
    "sandbox",
    "smtp",
    "signup",
    "login",
    "e2e",
    "automation",
    "verification"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://app.signinid.com",
  "bugs": {
    "url": "https://github.com/peerborough/signinid-node-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peerborough/signinid-node-sdk"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
